extends layout

block content
	h1 Log In 
	p Please Log in below
	form(method = 'post', action = '/users/login', enctype = 'multipart/form-data')
		.form-group
			label Username
			input.form-control(name="username", type="text")

		.form-group
			label Password
			input.form-control(name="password", type="password")

		input.btn.btn-default(name ='submit', type= 'submit', value='Log In')
